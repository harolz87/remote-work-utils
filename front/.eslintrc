{
    "extends": "airbnb",
    "parser": "babel-eslint",
    "rules": {
      "import/no-unresolved": "off",
      "no-prototype-builtins": "off",
      "linebreak-style": "off",
      "no-shadow": "off",
      "import/prefer-default-export": "off",
      "react/jsx-filename-extension": [
        1,
        {
          "extensions": [
            ".js",
            ".jsx"
          ]
        }
      ]
    },
    "globals": {
      "window": true,
      "document": true
    }
  }  